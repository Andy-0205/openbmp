LOGFILE=/var/log/openbmpd.log
OPENBMP_DB_HOST=${OPENBMP_DB_HOST:='localhost:3306'}
OPENBMP_DB_USER=${OPENBMP_DB_USER:='openbmp'}
OPENBMP_DB_PASSWORD=${OPENBMP_DB_PASSWORD:='openbmp'}
OPENBMP_DB_NAME=${OPENBMP_DB_NAME:='openBMP'}
OPENBMP_BUFFER=${OPENBMP_BUFFER:='10'}
OPENBMPD_OPTS="-l $LOGFILE -dburl tcp://$OPENBMP_DB_HOST -dbu $OPENBMP_DB_USER -dbp $OPENBMP_DB_PASSWORD -dbn ${OPENBMP_DB_NAME} -p 5000 -b ${OPENBMP_BUFFER}"
